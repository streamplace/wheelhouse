{
  "name": "wheelhouse-dev",
  "version": "0.0.1",
  "description": "Swiss Army knife for Javascript + Kubernetes",
  "main": "package.json",
  "scripts": {
    "test": "npm run lint && lerna run test && packages/wheelhouse/dist/wheelhouse.js --version",
    "postinstall": "lerna bootstrap",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "dev": "DEBUG='wheelhouse:*' lerna exec --concurrency=999 --no-sort npm run dev",
    "publish-prerelease": "npx lerna publish --exact --force-publish --npm-tag next --cd-version prerelease"
  },
  "bin": {
    "wh-dev": "run/wh-dev.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ],
    "*.css": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/streamplace/wheelhouse.git"
  },
  "keywords": [
    "Kubernetes"
  ],
  "author": "Streamplace",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/streamplace/wheelhouse/issues"
  },
  "homepage": "https://github.com/streamplace/wheelhouse#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "debug": "^2.6.8",
    "eslint": "^3.19.0",
    "eslint-plugin-prettier": "^2.1.0",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "json-stable-stringify": "^1.0.1",
    "lerna": "^2.5.1",
    "lint-staged": "^3.4.0",
    "milligram": "^1.3.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.2",
    "update-notifier": "^2.1.0"
  }
}
